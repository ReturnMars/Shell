{
  "name": "ShellMars",
  "private": true,
  "version": "alpha-0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:analyze": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "node scripts/lint.js check",
    "lint:fix": "node scripts/lint.js fix",
    "lint:check": "oxlint --max-warnings=0",
    "lint:report": "node scripts/lint.js report",
    "lint:html": "node scripts/lint.js html",
    "lint:open": "node scripts/lint.js open",
    "type-check": "vue-tsc --noEmit",
    "check": "npm run type-check && npm run lint:check"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.15",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@vicons/antd": "^0.13.0",
    "@xterm/addon-canvas": "^0.7.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-image": "^0.8.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "ansi-to-html": "^0.7.2",
    "naive-ui": "^2.43.1",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.3",
    "vfonts": "^0.0.3",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.15",
    "@tauri-apps/cli": "^2",
    "@types/node": "^24.9.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "autoprefixer": "^10.4.21",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "oxlint": "^1.24.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.93.2",
    "sass-embedded": "^1.93.2",
    "tailwindcss": "^4.1.15",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.1.11",
    "vue-tsc": "^3.1.1"
  },
  "volta": {
    "node": "24.10.0"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "oxlint --fix",
      "git add"
    ],
    "*.{ts,vue}": [
      "vue-tsc --noEmit"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run check"
    }
  }
}
